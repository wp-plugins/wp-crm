#: ../core/class_core.php:424 ../core/ui/crm_page_wp_crm_settings.php:170
msgid "Automation"
msgstr "Automatización"

#: ../core/class_core.php:425
msgid ""
"Use other attribute as components. Example: <b>[last_name], [rank]</b> will "
"become <b>Smith, Sgt.</b>"
